{
  "name": "@baller/i-ds",
  "version": "1.0.23",
  "description": "i-ds",
  "private": false,
  "type": "module",
  "files": [
    "dist",
    "*.d.ts",
    "*.css",
    "*.js",
    "./src/style.css",
    "./src/styles/ids-tokens.css"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "umd": "dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "keywords": [
    "i-ds",
    "vue",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier . --write",
    "build": "vue-tsc && vite build && npm run build:css",
    "build:css": "sass src/styles/tokens.scss dist/tokens.css --no-source-map --style compressed",
    "lint": "eslint . --fix --max-warnings=0",
    "generate:css": "node scripts/token-parser.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint 'src/**' --fix",
      "npm run test:staged"
    ]
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cypress": "^13.6.6",
    "cypress-real-events": "^1.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.11",
    "sass": "^1.71.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.1",
    "@rushstack/eslint-patch": "^1.7.2",
    "@vue-hero-icons/outline": "^1.7.2",
    "vite-plugin-dts": "^3.7.3",
    "vue-router": "^4.3.0",
    "vue-transition": "^0.1.3"
  }
}
