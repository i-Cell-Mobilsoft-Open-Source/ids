{
  "extends": [
    "plugin:vue/base",
    "plugin:vue/vue3-essential",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-typescript/recommended"
    //"plugin:@typescript-eslint/recommended"
    //"plugin:@typescript-eslint/eslint-recommended"
  ],
  "rules": {
    "max-attributes-per-line": "180"
  },
  "ignorePatterns": [
    "dist/**",
    "scripts/**",
    "vite.config.ts",
    "node_modules/**"
  ]
}